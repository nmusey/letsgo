package pages

templ Login() {
    <h1>LOGIN</h1>
    <form hx-post="/login">
        <label for="email">Email</label>
        <input type="email" name="email" id="email" />

        <label for="password">Password</label>
        <input type="password" name="password" id="password" />

        <input type="submit" value="Login" />
    </form>

    <a href="/register">Register</a>
}
